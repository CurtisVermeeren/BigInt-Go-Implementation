{
    "name": "BIGINT Go Implementation",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "go-app",
    "workspaceFolder": "/app",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.defaultProfile.linux": "zsh",
                "terminal.integrated.profiles.linux": {
                    "zsh": {
                        "path": "/bin/zsh"
                    }
                }
            },
            "extensions": [
                "golang.go",
                "codeium.codeium"
            ]
        }
    }
}